{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Profile Data",
	"description": "Data used to populate user profile",
	"type": "object",
	"properties" : {
		"title": {
			"type": "string"	
		},
		"personal": {
			"description" : "Details related to the personal individual",
			"type": "object",
			"properties": {
				"picture": {
					"type": "string"
				},
				"keybase": {
					"type": "string"
				},
				"bio": {
					"type": "string"
				},
				"resume": {
					"type": "string"
				},
				"cv": {
					"type": "string"
				},
				"contact" : {
					"description": "Methods to contact user",
					"type": "object",
					"properties": {
						"office": {
							"type": "string"
						},
						"phone": {
							"type": "string"
						},
						"email": {
							"type": "string"
						}
				},
				"social" : {
					"description": "Users online presense",
					"type": "object",
					"properties": {
						"github": {
							"type": "string"
						},
						"twitter": {
							"type": "string"
						},
						"linkedin": {
							"type": "string"
						},
						"google+": {
						   "type": "string"
						}	   
					}
				}
			}
		}

	}
}
